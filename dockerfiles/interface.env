# network
networkdriver="bridge"
subnet="10.0.0.0/24"

# service

# name
name="interface"

# volumes
volumepath="../env-restapi"
cronpath="../config/cron"
logspath="/tmp/logspath"
scriptspath="../scripts"

# build
context="."
dockerfile="interface.dockerfile"

entrypoint="./run.sh"
ipv4_address="10.0.0.33"
ports="5000:5000"

# healthcheck
restapiurl="localhost:5000"
interval="1m30s"
timeout="10s"
retries="3"
startperiod="40s"